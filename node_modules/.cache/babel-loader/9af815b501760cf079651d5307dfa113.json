YAAA,CAAc,CAAR,EAAQ,CAAA,MAAA,CAAR,CAAA;AAU/B,GAVN,E,CACE,YAAA,CAA2D,KAA3D,EAA2D;AAAtD,IAAA,KAAK,EAAC,qBAAgD;AAAzB,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,eAAA,IAAA,QAAA,CAAA,eAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAoB,GAA3D,C,EACA,YAAA,CAOM,KAPN,EAOM,IAPN,EAOM,CANJ,YAAA,CAKO,MALP,EAKO;AALD,IAAA,KAAK,EAAC,wBAKL;AAL+B,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,WAAA,IAAA,QAAA,CAAA,WAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAqB,CAAA,SAAA,CAArB,CAAA;AAKrC,GALP,E,CACE,U,kBACA,YAAA,CAA0B,OAA1B,EAA0B;;aAAV,KAAA,CAAA,QAAA,GAAQ,M;;AAAE,GAA1B,E,IAAA,E,GAAA,C,iBAAgB,KAAA,CAAA,Q,KACN,KAAA,CAAA,W,kBAAV,YAAA,CAAkN,IAAlN,EAAkN,UAAlN,EAAkN,C,UAAA,EAAhC,UAAgC,E,UAAA,CAAlN,C,mCACA,U,CAJF,E,EAAA,CAMI,CAPN,C,CAFF,CAYI,CAbN,C","sourcesContent":["<template>\r\n  <div class=\"login-popup__main-panel\">\r\n    <div class=\"buttons-container\" @click.stop=\"\">\r\n      <div class=\"google-login-button\" @click=\"redirectToLogin\"/>\r\n      <div>\r\n        <form class=\"temporary-account-form\" @submit.prevent=\"playAsGuest\">\r\n          <h4>or create a temporary user choosing a username</h4>\r\n          <input v-model=\"username\">\r\n          <h4 v-if=\"showWarning\">Attention! If you create a temporary user your data could be lost and you won't be able to login from a different device. If you are sure to proceed click <u>play as guest</u> again.</h4>\r\n          <button>Play as guest</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"LoginPopup\",\r\n  data(){\r\n    return {\r\n      showWarning: false,\r\n      username: null,\r\n    }\r\n  },\r\n  methods: {\r\n    playAsGuest(){\r\n      if(this.showWarning){\r\n        this.$emit(\"login\", this.username)\r\n        this.$emit(\"click\");\r\n      } else {\r\n        this.showWarning = true;\r\n      }\r\n    },\r\n    redirectToLogin(){\r\n      window.location.search = \"from_location=\"+window.location;\r\n      window.location.pathname = \"/auth/google\";\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../styles/global\";\r\n\r\n.login-popup__main-panel{\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background-color: rgba(0, 0, 0, 0.8);\r\n  z-index: 10;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n\r\n  @keyframes zoom-in {\r\n    0% {\r\n       transform: scale(0) rotate(1000deg);\r\n    }\r\n    100%{\r\n      transform: scale(1);\r\n    }\r\n  }\r\n\r\n  .buttons-container{\r\n    background: linear-gradient(45deg, $theme-color-dark, $theme-color-light);\r\n    border-radius: 10px;\r\n    width: 30%;\r\n    height: 80%;\r\n    display: grid;\r\n    grid-template-rows: auto 1fr;\r\n    animation: zoom-in 0.5s;\r\n\r\n    .google-login-button{\r\n      width: 191px;\r\n      height: 46px;\r\n      background-image: url(\"../assets/googleLoginButtons/btn_google_signin_dark_normal_web.png\");\r\n      justify-self: center;\r\n      align-self: center;\r\n      margin-top: 2%;\r\n\r\n      &:focus{\r\n        background-image: url(\"../assets/googleLoginButtons/btn_google_signin_dark_focus_web.png\");\r\n      }\r\n\r\n      &:active{\r\n        background-image: url(\"../assets/googleLoginButtons/btn_google_signin_dark_pressed_web.png\");\r\n      }\r\n    }\r\n\r\n    .temporary-account-form{\r\n\r\n      display: flex;\r\n      flex-flow: column;\r\n      align-items: center;\r\n      justify-content: center;\r\n\r\n      h4{\r\n        font-variant: normal;\r\n        color: white;\r\n        text-align: center;\r\n        font-size: 3vh;\r\n        width: 80%;\r\n      }\r\n\r\n      input{\r\n        border: none;\r\n        font-size: 5vh;\r\n        width: 90%;\r\n        text-align: center;\r\n\r\n        &:focus{\r\n          outline: none;\r\n        }\r\n      }\r\n\r\n      button{\r\n        margin: 2%;\r\n        border: 1px solid white;\r\n        background-color: transparent;\r\n        padding: 1% 2% 1% 2%;\r\n        color: white;\r\n        font-size: 5vh;\r\n        transition: all 0.5s;\r\n\r\n        &:hover{\r\n          background-color: white;\r\n          color: $theme-color-dark;\r\n        }\r\n\r\n        &:focus{\r\n          outline: none;\r\n        }\r\n\r\n      }\r\n\r\n    }\r\n  }\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}