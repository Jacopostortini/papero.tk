 top: 40px;\r\n  animation: fade-in 1s;\r\n  border-radius: 20%;\r\n  width: 90%;\r\n  height: auto;\r\n  text-align: center;\r\n  color: black;\r\n  overflow: hidden;\r\n\r\n  @media (max-width: 500px){\r\n    position: relative !important;\r\n    top: 0;\r\n    margin-top: 10px;\r\n    margin-bottom: 5px;\r\n    &:hover{\r\n      transform: scale(1) !important;\r\n    }\r\n  }\r\n\r\n\r\n  &:hover{\r\n    transform: scale(.8);\r\n  }\r\n\r\n  .background_image{\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 0;\r\n  }\r\n\r\n  .title_image{\r\n    position: absolute;\r\n    top: 5px;\r\n    left: 0;\r\n    width: 80%;\r\n    margin-left: 10%;\r\n    z-index: 1;\r\n  }\r\n\r\n  .preview_image{\r\n    position: absolute;\r\n    bottom: 25px;\r\n    left: 0;\r\n    width: 60%;\r\n    margin-left: 20%;\r\n    z-index: 2;\r\n  }\r\n\r\n\r\n  &.not_available{\r\n    .title_image{\r\n      top: 35%;\r\n    }\r\n  }\r\n\r\n  &.papero_image{\r\n    border-radius: 0;\r\n    width: 100% !important;\r\n    margin: 0 !important;\r\n    top: calc(min(27vw, 350px));\r\n\r\n    .title_image{\r\n      width: 100%;\r\n      top: 0;\r\n      margin: 0;\r\n\r\n    }\r\n\r\n    .background_image{\r\n      display: none;\r\n    }\r\n\r\n    &:hover{\r\n      transform: scale(1) !important;\r\n    }\r\n  }\r\n  .coming_soon_video{\r\n    width: 100%;\r\n    position: absolute;\r\n    top: 30%;\r\n    left: 0;\r\n    margin: 0;\r\n    img{\r\n      width: 80%;\r\n      margin: auto;\r\n    }\r\n  }\r\n}\r\n\r\n\r\n\r\n</style>"],"sourceRoot":""}]}