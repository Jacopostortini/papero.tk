         this.username = response.data.username;\r\n          this.logged = response.data.google_signed_in;\r\n        }\r\n      });\r\n  },\r\n  methods: {\r\n    redirectToGoogle(){\r\n      let from_location = window.location;\r\n      window.location.href = '/auth/google?from_location='+from_location;\r\n    },\r\n    logout(){\r\n      axios.get(urls.logoutUrl);\r\n      this.username = null;\r\n      this.logged = false;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../styles/global\";\r\n\r\n.user-hamburger-menu__main-panel{\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width:calc(25vw + 30px);\r\n  max-width: 250px;\r\n  height: 100%;\r\n  display: flex;\r\n  padding: 2% 2% 10% 2%;\r\n  flex-flow: row;\r\n  transition: all 0.5s;\r\n  z-index: 1;\r\n  border-right: 1px solid white;\r\n  border-bottom: 1px solid white;\r\n  background-color: #fd8c13;\r\n  &.hidden{\r\n    transform: translateX(-100%);\r\n  }\r\n  .user-hamburger-menu__menu{\r\n    display: flex;\r\n    flex-flow: column;\r\n    align-items: center;\r\n    text-align: center;\r\n    .home-button{\r\n      width: 90%;\r\n    }\r\n    .logged-menu{\r\n      margin-top: 10%;\r\n      display: flex;\r\n      flex-flow: column;\r\n      align-items: center;\r\n      color: white;\r\n      font-size: 20px;\r\n\r\n      div{\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: space-evenly;\r\n        align-items: center;\r\n        flex-wrap: wrap;\r\n      }\r\n    }\r\n    .not-logged-menu{\r\n      margin-top: 10%;\r\n      display: flex;\r\n      flex-flow: column;\r\n      align-items: center;\r\n      color: white;\r\n\r\n      font-size: 20px;\r\n      p{\r\n        margin: 10px;\r\n      }\r\n    }\r\n  }\r\n  .user-hamburger-menu__icon{\r\n    position: absolute;\r\n    width: calc(8% + 10px);\r\n    height: min-content;\r\n    right: calc(-10% - 20px);\r\n    top: 20px;\r\n    transition: all 0.5s;\r\n    z-index: 5;\r\n    img{\r\n      float: left;\r\n      width: 100%;\r\n      height: auto;\r\n    }\r\n    &.rotated{\r\n      transform: rotate(90deg);\r\n    }\r\n  }\r\n}\r\n</style>"]}]}